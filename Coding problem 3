let age = prompt("Enter your age");
if (age < 5){
  console.log("You're  a Toddler.");
} else if (age < 12 ){
  console.log("You're a Child.")
} else if (age < 19){
  console.log("You are a Teenager.")
} else if (age < 35){
  console.log("You're a Young Adult.")
} else if (age < 60)  {
  console.log("you're a Middle Adult.")
} else {
  console.log("congratulation you're now a Senior")
}
